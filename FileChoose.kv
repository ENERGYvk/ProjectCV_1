#:kivy 2.1.0



<MyWidget>:

    rows:2
    id:my_widget

    GridLayout:
        cols:2
        size_hint:1,1

        Image:
            id:image
            source:""

        TextInput:
            margin:0
            size_hint_x: 0.5
            id:textbox

    FileChooserIconView:
        id:filechooser
        on_selection:my_widget.selected(filechooser.selection)


